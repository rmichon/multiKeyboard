#!/bin/bash

faust2api -ios -polyvoices 16 -poly2 -midi -nodoc synthMulti.dsp
#faust2api -ios -polyvoices 12 -midi -nodoc noPoly.dsp
#faust2api -ios -polyvoices 12 -midi -nodoc midiOnly.dsp
unzip dsp-faust.zip
cp dsp-faust/DspFaust.cpp ../iOS/iOSKeyboard
cp dsp-faust/DspFaust.h ../iOS/iOSKeyboard
rm -r dsp-faust
rm dsp-faust.zip